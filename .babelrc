{
	"presets": ["babel-preset-expo"],
	"env": {
		"development": {
			"plugins": ["transform-react-jsx-source"]
		}
	},
	"plugins": [
		[
			"module-resolver",
			{
				"alias": {
					"Parser": "./api/parsers",
					"Module": "./modules",
					"Interfaces": "./api/interfaces",
					"UI": "./ui/components",
					"Styles": "./ui/styles"
				}
			}
		]
	]
}
